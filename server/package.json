{
  "name": "server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "babel-watch index.js",
    "build": "babel -d ./build/src ./src -s; babel index.js -d ./build; cp -a src/config ./build/src",
    "migrate": "node_modules/.bin/sequelize db:migrate"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-watch": "^2.0.7",
    "eslint": "^4.6.1"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "binary-parser": "^1.3.0",
    "express": "^4.16.1",
    "immutable": "^3.8.2",
    "jsonwebtoken": "^7.2.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "socket.io": "^2.1.0",
    "sqlite": "^2.8.0",
    "uuid": "^3.2.1"
  }
}
